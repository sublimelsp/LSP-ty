{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>sublimelsp/renovate-preset"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^plugin/constants\\.py$/"
      ],
      "matchStrings": [
        "SERVER_VERSION = \"(?<currentValue>.+?)\""
      ],
      "datasourceTemplate": "github-releases",
      "packageNameTemplate": "astral-sh/ty"
    }
  ]
}
